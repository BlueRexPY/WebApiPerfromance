    "panels": [
        {
            "title": "Active Wait Events by Type",
            "description": "Real-time distribution of PostgreSQL wait events across all backend types (IO, Lock, LWLock, Client, etc.)",
            "type": "timeseries",
            "gridPos": {
                "h": 10,
                "w": 12,
                "x": 0,
                "y": 0
            },
            "datasource": {
                "type": "prometheus",
                "uid": "${datasource}"
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "custom": {
                        "lineWidth": 2,
                        "fillOpacity": 30,
                        "stacking": {
                            "mode": "normal"
                        },
                        "spanNulls": true
                    }
                },
                "overrides": []
            },
            "options": {
                "tooltip": {
                    "mode": "multi",
                    "sort": "desc"
                },
                "legend": {
                    "displayMode": "table",
                    "placement": "right",
                    "calcs": [
                        "mean",
                        "max",
                        "last"
                    ]
                }
            },
            "targets": [
                {
                    "expr": "sum by (wait_event_type) (pg_stat_activity_wait_events_count{state=\"active\"})",
                    "legendFormat": "{{wait_event_type}}"
                }
            ]
        },
